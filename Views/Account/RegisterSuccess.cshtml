@{
    ViewBag.Title = AccountStrings.ResourceManager.Get("RegisterTitle");
}  
<div class="container">    
	<div class="row bk-main"> 	      
        <div class="col-lg-12 col-md-12">
			<h1 class="page-header">@ViewBag.Title</h1>				           
		</div>
        <div class="col-lg-12 col-md-12">                     
            <h3 style="color:#8c8c8c; margin-top:10px;">@AccountStrings.ResourceManager.Get("RegisterEmailNotConfirmed")</h3>
            <h4>@AccountStrings.ResourceManager.Get("RegisterEmailNote")</h4>
        </div>
        <div class="clearfix" style="margin-bottom:10px;"></div>	
        <div class="col-lg-12 col-md-12">         
            @if (ViewBag.ReturnUrl != null)
            {            
                 <button name="submit" id="submit" value="" type="submit" class="btn btn-large btn-primary" onclick="window.location='@ViewBag.ReturnUrl';">@AccountStrings.ResourceManager.Get("Register_MainFormLink")</button>        
            }
            else
            {
                <button name="submit" id="submit" value="" type="submit" class="btn btn-large btn-primary" onclick="window.location='@Url.Action("index", "welcome")';">@AccountStrings.ResourceManager.Get("Register_MainFormLink")</button>                    
            }
        </div>
    </div>
</div>